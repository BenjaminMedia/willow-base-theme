{
    "name": "bonnier/willow-base-theme",
    "description": "Base Willow theme for WordPress",
    "type": "wordpress-theme",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type":"vcs",
            "url":"https://github.com/junaidbhura/composer-wp-pro-plugins"
        },
        {
            "type": "package",
            "package": {
                "name": "junaidbhura/advanced-custom-fields-pro",
                "version": "5.7.2",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://www.advancedcustomfields.com"
                },
                "require": {
                    "junaidbhura/composer-wp-pro-plugins": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "junaidbhura/polylang-pro",
                "version": "2.3.8",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://www.polylang.pro"
                },
                "require": {
                    "junaidbhura/composer-wp-pro-plugins": "*"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1",
        "junaidbhura/advanced-custom-fields-pro": "*",
        "junaidbhura/polylang-pro": "*",
        "league/fractal": "^0.17.0",
        "wpackagist-plugin/amazon-s3-and-cloudfront": "^1.4",
        "wpackagist-plugin/user-role-editor": "^4.44",
        "bonnier/willow-mu-plugins": "^1.1",
        "bonnier/contenthub-editor": "^3.0",
        "benjaminmedia/wp-cxense": "^2.1",
        "benjaminmedia/wp-site-manager": "^1.0",
        "bonnier/wp-bonnier-cache": "^2.0",
        "bonnier/wp-bonnier-redirect": "^2.0",
        "bonnier/wp-bonnier-some": "^2.0",
        "bonnier/wp-focal-point": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Bonnier\\Willow\\Base\\": "src",
            "Bonnier\\Willow\\Base\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.1.*",
        "lucatume/wp-browser": "^2.1",
        "johnpbloch/wordpress": "^4.9"
    },
    "scripts": {
        "unit": "vendor/bin/codecept run Unit"
    }
}
